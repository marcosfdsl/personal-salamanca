---
import Layout from "../layouts/Layout.astro";

import AntonioyZoraida from "../img/AntonioyZoraida.webp";
import AlbertoFernandezdeSalamancaCastilla from "../img/AlbertoFernándezdeSalamancaCastilla.webp";
import AlbertoFernandezdeSalamancaSaenzdeVizmanos from "../img/AlbertoFernándezdeSalamancaSaenzdeVizmanos.webp";
import AlbertoFernandezdeSalamancaMontaner from "../img/AlbertoFernándezdeSalamancaMontaner.webp";
